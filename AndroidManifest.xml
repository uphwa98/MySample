<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shim.mysample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".LocalService" >
        </service>
        <activity android:name=".PendingIntentNAlarmManager" />
        <service android:name=".PendingIntentNAlarmManager$AlarmService" />
        <broadcast android:name=".PendingIntentNAlarmManager$AlarmBroadcast" />
        <activity android:name=".PendingIntentNAlarmManager$AlarmActivity" />
    </application>

</manifest>
